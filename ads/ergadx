function checkForAdsReadyToRefresh(){const slotsToRefresh=[];if(!browserFocus){setTimeout(checkForAdsReadyToRefresh,500);return;}
for(const slot in adSlotMap){if(adSlotMap.hasOwnProperty(slot)){if(isDebugEnabled){}
